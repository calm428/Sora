  "scripts": {
    "build": "run-s \"build:*\"",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "cleanup": "rimraf .cache ./public/build ./public/entry.worker.js ./build",
    "dev": "run-p \"dev:*\"",
    "dev:remix": "cross-env NODE_ENV=development remix dev --manual",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "format-code": "pnpm run prettier:fix & pnpm run lint:fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prebuild": "pnpm cleanup",
    "prepare": "husky install",
    "prettier": "prettier app --check",
    "prettier:fix": "prettier app --write",
    "start": "dotenv -e .env remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
