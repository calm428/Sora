{
  "private": true,
  "name": "sora",
  "description": "An app built with Remix",
  "license": "GPL-3.0",
  "scripts": {
    "cleanup": "rimraf .cache ./public/build ./app/styles/tailwind.css",
    "build": "pnpm prod:css && pnpm build:worker && remix build",
    "build:css": "postcss styles/**/*.css --dir app/styles",
    "prod:css": "pnpm build:css",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "concurrently \"pnpm dev:css\" \"pnpm dev:worker\" \"remix dev\"",
    "dev:css": "pnpm build:css --watch",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "prebuild": "pnpm cleanup",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prepare": "husky install",
    "format:write": "prettier --write \"**/*.{ts,tsx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx}\" --cache",
    "format-code": "pnpm run prettier:fix & pnpm run lint:fix",
    "prettier": "prettier app --check",
    "prettier:fix": "prettier app --write"
  },
